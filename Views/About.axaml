				<Label Margin="10,0,0,0">Kristian Solovic</Label>
				<Label Margin="10,0,0,0">Marc Cendros Riutort</Label>
				<Label Margin="10,0,0,0">Martin Olivera</Label>
				<Label Margin="10,0,0,0">Toni Vera Gutierrez</Label>
				<Label Margin="10,0,0,0">Victor Fernandez Riomao</Label>
			</StackPanel>
		</Grid>
		<Rectangle Height="50" Grid.Row="1"></Rectangle>
		
	</Grid>
</UserControl>
